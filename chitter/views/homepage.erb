<html>
  <head>
  <title>Welcome to Chitter!</title>
  </head>
    <body>
      <form action="/login" method="GET">
          <p><input type="submit" value="login to chitter"></p>
      </form>
            <form action="/signup" method="GET">
          <p><input type="submit" value="signup to chitter"></p>
      </form>
    </body>
  <body>
    <h1>Chitter</h1>
    <h2>Check out the latest peeps...</h2>
    <%@peeps.each do |peep| %>
    <div>
      <p> <%=peep.content%> --- <%=peep.username%> --- peeped at <%=peep.time%></p>
    </div>
    <%end%>
  </body>
</html>

